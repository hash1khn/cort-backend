  invoices                   invoices[]
  routes                     routes[]
  users                      public_users[]
  vehicle_whitelists         vehicle_whitelists[]

  @@schema("public")
}

model drivers_profile {
  user_id            String                    @id @db.Uuid
  cnic_number        String?                   @unique @db.VarChar(20)
  license_number     String?                   @db.VarChar(20)
  driver_type        driver_type
  current_vehicle_id Int?
  is_online          Boolean?                  @default(false)
  current_location   Unsupported("geography")?
  last_updated       DateTime?                 @db.Timestamp(6)
  vehicles           vehicles?                 @relation(fields: [current_vehicle_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users              public_users              @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([current_location], map: "idx_driver_location", type: Gist)
  @@schema("public")
}

model employee_route_assignments {
  user_id        String       @id @db.Uuid
  route_id       Int?
  pickup_stop_id Int?
  route_stops    route_stops? @relation(fields: [pickup_stop_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  routes         routes?      @relation(fields: [route_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users          public_users @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@schema("public")
}

model invoice_line_items {
  id          Int       @id @default(autoincrement())
  invoice_id  Int?
  description String?   @db.VarChar(255)
  quantity    Decimal?  @db.Decimal(10, 2)
  unit_price  Decimal?  @db.Decimal(10, 2)
  total_price Decimal?  @db.Decimal(10, 2)
  category    String?   @db.VarChar(50)
  metadata    Json?
  invoices    invoices? @relation(fields: [invoice_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@schema("public")
}

model invoices {
  id                 Int                  @id @default(autoincrement())
  company_id         Int?
  invoice_number     String               @unique @db.VarChar(50)
  billing_month      String?              @db.VarChar(20)
  generated_at       DateTime?            @default(now()) @db.Timestamp(6)
  total_amount       Decimal?             @db.Decimal(12, 2)
  status             String?              @default("DRAFT") @db.VarChar(20)
  pdf_url            String?
  invoice_line_items invoice_line_items[]
  companies          companies?           @relation(fields: [company_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@schema("public")
}

model route_stops {
  id                         Int                          @id @default(autoincrement())
  route_id                   Int?
  name                       String                       @db.VarChar(100)
  sequence_order             Int
  location                   Unsupported("geography")?
  morning_eta                DateTime?                    @db.Time(6)
  evening_eta                DateTime?                    @db.Time(6)
  employee_route_assignments employee_route_assignments[]
  routes                     routes?                      @relation(fields: [route_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@schema("public")
}

model routes {
  id                         Int                          @id @default(autoincrement())
  name                       String                       @db.VarChar(100)
  company_id                 Int?
  assigned_vehicle_id        Int?
  assigned_driver_id         String?                      @db.Uuid
  created_at                 DateTime?                    @default(now()) @db.Timestamp(6)
  employee_route_assignments employee_route_assignments[]
  route_stops                route_stops[]
  users                      public_users?                @relation(fields: [assigned_driver_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  vehicles                   vehicles?                    @relation(fields: [assigned_vehicle_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  companies                  companies?                   @relation(fields: [company_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shuttle_trips              shuttle_trips[]

  @@schema("public")
}

model shuttle_boarding_logs {
  id              Int            @id @default(autoincrement())
  shuttle_trip_id Int?
  employee_id     String?        @db.Uuid
  scanned_at      DateTime?      @db.Timestamp(6)
  status          String?        @default("BOARDED") @db.VarChar(20)
  drop_off_at     DateTime?      @db.Timestamp(6)
  users           public_users?  @relation(fields: [employee_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shuttle_trips   shuttle_trips? @relation(fields: [shuttle_trip_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@schema("public")
}

model shuttle_trips {
  id                    Int                     @id @default(autoincrement())
  route_id              Int?
  driver_id             String?                 @db.Uuid
  trip_date             DateTime?               @default(dbgenerated("CURRENT_DATE")) @db.Date
  direction             shuttle_direction
  status                String?                 @default("STARTED") @db.VarChar(20)
  started_at            DateTime?               @db.Timestamp(6)
  completed_at          DateTime?               @db.Timestamp(6)
  shuttle_boarding_logs shuttle_boarding_logs[]
  users                 public_users?           @relation(fields: [driver_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  routes                routes?                 @relation(fields: [route_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([trip_date], map: "idx_shuttle_trips_date")
  @@schema("public")
}

