  @@schema("public")
}

model public_users {
  id                                                        String                      @id @db.Uuid
  full_name                                                 String                      @db.VarChar(100)
  email                                                     String                      @unique @db.VarChar(255)
  phone                                                     String?                     @db.VarChar(20)
  role                                                      user_role
  company_id                                                Int?
  employee_id                                               String?                     @db.VarChar(50)
  status                                                    String?                     @default("ACTIVE") @db.VarChar(20)
  created_at                                                DateTime?                   @default(now()) @db.Timestamp(6)
  chauffeur_bookings_chauffeur_bookings_driver_idTousers    chauffeur_bookings[]        @relation("chauffeur_bookings_driver_idTousers")
  chauffeur_bookings_chauffeur_bookings_passenger_idTousers chauffeur_bookings[]        @relation("chauffeur_bookings_passenger_idTousers")
  drivers_profile                                           drivers_profile?
  employee_route_assignments                                employee_route_assignments?
  routes                                                    routes[]
  shuttle_boarding_logs                                     shuttle_boarding_logs[]
  shuttle_trips                                             shuttle_trips[]
  companies                                                 companies?                  @relation(fields: [company_id], references: [id], onUpdate: NoAction)
  users                                                     auth_users                  @relation(fields: [id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([role], map: "idx_users_role")
  @@map("users")
  @@schema("public")
}

model vehicle_whitelists {
  id                    Int        @id @default(autoincrement())
